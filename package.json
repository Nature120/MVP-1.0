    "clean": "yarn clean:ios && cd android && ./gradlew clean",
    "clean:android": "yarn clean:js && cd android && ./gradlew clean",
    "clean:ios": "watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install && cd ios && rm -rf Pods && rm -rf build && pod install ",
